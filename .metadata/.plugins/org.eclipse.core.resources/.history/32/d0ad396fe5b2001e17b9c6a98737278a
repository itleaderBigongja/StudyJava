package SamplePackage;

public class MethodInStringWrapper {

	public static void main(String[] args) {
		
		// 문자의 마지막 인덱스 찾기
		String str_2 = new String("String Wrapper 클래스 기능이 너무 많아요..ㅠㅠ");
		// 문자열의 처음 인덱스 찾기
		System.out.println("문자열의 첫 번째 인덱스 : " + str_2.charAt(0) + " 위치 입니다.");
		// 문자열의 마지막 인덱스 찾기
		System.out.println("문자열의 마지막 인덱스 : " + str_2.charAt(str_2.length() -1) + " 위치 입니다.");
	}
}


