package SamplePackage;

public class StringClass_2 {

	public static void main(String[] args) {
	
		String str = "오늘의 공부는 indexOf 입니다.";
		
		System.out.println("첫 번째 문자열 찾기 : " + str.indexOf('공', 0)); // 찾기 시작위치 0부터
		System.out.println("첫 번째 문자열 찾기 : " + str.indexOf('부', 6)); // 찾기 시작위치 6부터 
	}
}





