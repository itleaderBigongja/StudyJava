package SamplePackage;

public class StringClass_2 {

	public static void main(String[] args) {
	
		String str = "오늘의 공부는 indexOf 입니다.";
		
		System.out.println("문자열에서 첫 번째 특정 문자열 시작위치는? : " + str.indexOf("indexOf", 2));
		System.out.println("문자열에서 두 번째 특정 문자열 시작위치는? : " + str.indexOf("공부", 3));
	}
}





