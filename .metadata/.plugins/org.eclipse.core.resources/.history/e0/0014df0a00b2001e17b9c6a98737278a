package SamplePackage;

public class SampleClass {


	public static void main(String[] args) {
		

		int int_1;				// 방법_1 = int형 변수의 생성 
		int int_2 = 0;			// 방법_2 = int형 변수의 생성 및 0으로 초기화
		
		
		float float_1;			// 방법_1 = float형 변수의 생성 
		float float_2 = 0;		// 방법_2 = float형 변수의 생성 및 0으로 초기화
		
		
		char char_1;			// 방법_1 = char형 변수의 생성  
		char char_2 = ' ';		// 방법_2 = char형 변수의 생성 및 공백으로 초기화
		
		
		String string_1;		// 방법_1 = String형 변수의 생성 
		String string_2 = "";	// 방법_2 = String형 변수의 생성 및 공백으로 초기화
		
	}

}
